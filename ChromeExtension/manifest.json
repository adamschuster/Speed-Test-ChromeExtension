
{
  "manifest_version": 2,

  "name": "Internet Speed Test",
  "description": "This extension will test the users internet speed",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icons/icon4.png",
    "default_popup": "popup.html",
    "default_title": "Atomic Internet Speed Test"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"https://*/",
	"http://*/"
    ]
}